package com.capgemini;

class parent
{
	int a,b,c;
	void show()
	{
		a=100;
		b=200;
		c=a+b;
		System.out.println(c);
	}
}

class child
{
	int a,b,c;
	void show()
	{
		a=300;
		b=400;
		c=a+b;
		System.out.println(super.c);
	}
}
	

public class This_Super_Practice {

	public static void main(String[] args) {
		child c=new child();
		c.show();

	}

}
